<p id="notice"><%= notice %></p>

<h1>Rockets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Astronauts</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<iframe src="https://giphy.com/embed/S99cgkURVO62qemEKM" width="380" height="270" align = 'right' frameBorder="0" class="giphy-embed" allowFullScreen></iframe><p><a href="https://giphy.com/gifs/nasa-nasareactions-S99cgkURVO62qemEKM"></a></p>
  <tbody>
    <% @engines.each do |engine| %>
      <tr>
        <td><%= engine.name %></td>
        <td><%= engine.model %></td>
        <td><%= engine.astronauts %></td>
        <td>&nbsp;</td>
        <td><%= link_to 'Show', engine, class: 'linkButton2' %></td>
        <td>&nbsp;</td>
        <td><%= link_to 'Edit', edit_engine_path(engine), class: 'linkButton2' %></td>
        <td>&nbsp;</td>
        <td><%= link_to 'Destroy', engine, method: :delete, data: { confirm: 'Are you sure?' }, class: 'linkButton2' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rocket', new_engine_path, class: 'linkButton' %>
<br>
<a href = '/engines.json' target = '_blank'><div class="linkButton">See Json</div></a>